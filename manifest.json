{
  "name": "Thanos Rage",
  "version": "1.0",
  "description": "Snap the fingers and bring chrome tabs in balance, just like thanos did",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "declarativeContent"
  ],
  "page_action": {
    "default_popup": "./popup/popup.html",
    "default_icon": {
      "16": "./images/16.png",
      "32": "./images/32.png",
      "48": "./images/48.png",
      "128": "./images/128.png"
    }
  },
  "icons": {
    "16": "./images/16.png",
    "32": "./images/32.png",
    "48": "./images/48.png",
    "128": "./images/128.png"
  },
  "manifest_version": 2
}